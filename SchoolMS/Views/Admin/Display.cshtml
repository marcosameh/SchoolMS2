@model IEnumerable<SchoolMS.Models.Admin>
@{
    ViewBag.Title = "Display";
    Layout = "~/Views/DashboardLayout.cshtml";
}
    <head>
        <link href="~/Content/font-awesome.css" rel="stylesheet" />
    </head>

    <h1>
        <a href="/Account/AdminRegistration" class="text-danger">Create New Employee</a>
        
    </h1>
<link href="~/fonts/font-awesome-4.7.0/css/font-awesome.css" rel="stylesheet" />

<center>
    <table class="table table-hover table-dark">
        <tr class="font-weight-bold ">
            <td>Id<span class=""></span></td>
            <td>Name</td>
            <td>Salary</td>

            <td>Edit</td>
            <td>Delete</td>
        </tr>
        @{
            foreach (var item in Model)
            {
                <tr >
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Salary</td>


                    <td><a href="/Admin/Edit/@item.Id" class="btn btn-success" >Edit <span class="fa fa-edit"></span></a></td>
                    <td><a href="/Admin/Delete/@item.Id" class="btn btn-danger">Delete <span class="fa fa-trash"></span></a></td>
                </tr>
            }
        }
    </table>
</center>


